<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.jamesd.passwordmanager.Controllers.PasswordHomeController">
    <children>
        <VBox layoutX="108.0" layoutY="100.0" prefHeight="226.0" prefWidth="385.0">
            <children>
                <Label text="Are you sure you want to delete the selected passwords?">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Label>
                <Button fx:id="confirmDeleteButton" mnemonicParsing="false" onAction="#confirmDelete" prefHeight="24.0" prefWidth="68.0" text="Delete" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets left="160.0" top="100.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelDelete" prefHeight="24.0" prefWidth="68.0" text="Cancel">
                    <VBox.margin>
                        <Insets left="160.0" top="10.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
