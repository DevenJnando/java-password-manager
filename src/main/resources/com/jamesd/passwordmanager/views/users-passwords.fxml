<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="510.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jamesd.passwordmanager.Controllers.PasswordHomeController">
   <left>
      <JFXDrawer fx:id="menuDrawer" defaultDrawerSize="200.0" prefHeight="400.0" prefWidth="193.0" BorderPane.alignment="CENTER" />
   </left>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <JFXHamburger fx:id="menuHamburger" alignment="CENTER" layoutX="949.0" layoutY="7.0" onMouseClicked="#onHamburgerClick" prefHeight="200.0" prefWidth="100.0" />
         </children>
      </HBox>
   </top>
   <center>
      <StackPane prefHeight="385.0" prefWidth="681.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="25.0" right="200.0" />
         </BorderPane.margin>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="62.0" prefWidth="663.0">
                     <children>
                        <Label fx:id="savedPasswordsLabel" prefHeight="31.0" prefWidth="271.0" text="Saved Passwords:">
                           <font>
                              <Font size="26.0" />
                           </font>
                        </Label>
                        <JFXButton fx:id="deletePasswordsButton" alignment="TOP_RIGHT" buttonType="RAISED" mnemonicParsing="false" onAction="#deletePasswords" prefHeight="41.0" prefWidth="48.0" text=" ">
                           <HBox.margin>
                              <Insets left="275.0" />
                           </HBox.margin>
                        </JFXButton>
                     </children>
                  </HBox>
                  <TableView fx:id="passwordTableView" prefHeight="362.0" prefWidth="607.0">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </VBox>
         </children>
      </StackPane>
   </center>
</BorderPane>
